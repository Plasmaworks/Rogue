module Entity

class UIConsoleFont : UIFont [singleton]
  METHODS
    method cloned->UIConsoleFont
      return this

    method draw( text:String, position:XY )
      UIConsoleViewCanvas.cursor = position
      UIConsoleViewCanvas.print text

    method height->Int32
      return 1

    method measure( ch:Character )->RealXY
      return RealXY(1,1)
endClass
